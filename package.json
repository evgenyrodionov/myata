    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.css": [
        "prettier --write",
        "git add"
      ],
      "*.{js,jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{md,json}": [
        "prettier --write --config .prettierrc",
        "git add"
      ]
    }
  }
